package Ex09;

public class Variable {
	
	
	// 인스턴스 변수
    String color;
    String model;
    int year;

    // 생성자
    public Variable(String color, String model, int year) {
        this.color = color;
        this.model = model;
        this.year = year;
    }
	
    public void displayInfo() {
        System.out.println("Color: " + color);
        System.out.println("Model: " + model);
        System.out.println("Year: " + year);
    }

}
